@page
@using HE176084_MinhBT_A3.Pages.AppUser
@model LoginModel

<h2>Login</h2>

<form method="post">
    <div class="form-group">
        <label asp-for="LoginUser.Email"></label>
        <input asp-for="LoginUser.Email" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="LoginUser.Password"></label>
        <input asp-for="LoginUser.Password" type="password" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Login</button>
</form>
